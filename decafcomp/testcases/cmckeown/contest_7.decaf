extern func print_int(int) void;
extern func print_string(string) void;

package Test {
    func main() int {
        var x int;
        x = 19;

        // shifting
        print_int(x << 1);
        print_string("\n");

        print_int(x >> 1);
        print_string("\n");
        
        print_int(x >> 1 >> 1);
    }
}